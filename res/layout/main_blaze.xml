<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:padding="10px">
   
	<FrameLayout android:id="@+id/frameLayout1"
	        android:layout_width="160px" android:layout_height="135px"
	        android:orientation="horizontal" android:layout_alignParentLeft="true"
	        android:layout_alignParentBottom="true"
            android:layout_marginLeft="10px"
	        />
	<FrameLayout android:id="@+id/frameLayout2"
	        android:layout_width="160px" android:layout_height="135px"
	        android:layout_toRightOf="@id/frameLayout1"
            android:layout_alignTop="@id/frameLayout1"
            android:layout_marginLeft="10px"
	        android:orientation="horizontal"
	        />
	<FrameLayout android:id="@+id/frameLayout3"
	        android:layout_width="160px" android:layout_height="135px"
	        android:layout_toRightOf="@id/frameLayout2"
            android:layout_alignTop="@id/frameLayout1"
            android:layout_marginLeft="10px"
	        android:orientation="horizontal"
	        />
	        
	<FrameLayout android:id="@+id/frameLayout4"
	        android:layout_width="160px" android:layout_height="135px"
	        android:layout_toRightOf="@id/frameLayout3"
	         android:layout_alignTop="@id/frameLayout1"
            android:layout_marginLeft="10px"
	        android:orientation="horizontal"
	        />
	        
	<FrameLayout android:id="@+id/frameLayout5"
	        android:layout_width="160px" android:layout_height="135px"
	        android:layout_toRightOf="@id/frameLayout4"
	         android:layout_alignTop="@id/frameLayout1"
            android:layout_marginLeft="10px"
	        android:orientation="horizontal"
	        />
	        
	<FrameLayout android:id="@+id/frameLayout6"
	        android:layout_width="320px" android:layout_height="360px"
	        android:layout_above="@id/frameLayout2"
	        android:layout_alignLeft="@id/frameLayout2"
	        android:layout_marginLeft="80px"
            android:layout_marginBottom="40px"
	        android:orientation="horizontal"
	        />
	
	<FrameLayout android:id="@+id/frameLayout7"
	        android:layout_width="160px" android:layout_height="90px"
	        android:layout_alignTop="@id/frameLayout6"
	        android:layout_toRightOf="@id/frameLayout6"
	        android:layout_marginLeft="10px"
	        android:orientation="horizontal"
	        />
		
	<FrameLayout android:id="@+id/frameLayout8"
	        android:layout_width="160px" android:layout_height="90px"
	        android:layout_alignBottom="@id/frameLayout6"
	        android:layout_toRightOf="@id/frameLayout6"
	        android:layout_marginLeft="10px"
	        android:orientation="horizontal"
	        />
	
	<Button android:id="@+id/gobutton"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:enabled="false"
	    android:text="Start" />
		
	<Button android:id="@+id/rotateButton"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentRight="true"
	    android:layout_marginRight="100px"
	    android:visibility="invisible"
	    android:enabled="false"
	    android:text="Rotate" />
	
	<Button android:id="@+id/reflectionButton"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentRight="true"
	    android:layout_below="@id/rotateButton"
	    	   android:layout_marginRight="100px"
	    android:visibility="invisible"
	    android:text="Reflect" />
	
	<Button android:id="@+id/rotateAllButton"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentRight="true"
	    android:layout_below="@id/reflectionButton"
	   	android:layout_marginRight="100px"
	   	android:visibility="invisible"
	    android:text="Rotate All" />
	    
	<SurfaceView
		android:layout_width="32px" 
	    android:layout_height="24px" 
	    android:layout_below="@id/gobutton"
		android:id="@+id/surfaceVideo"
		/>
	
	<TextView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/gobutton"
	    android:layout_marginTop="10px"
	    android:id="@+id/textFPS1"
	/>
	
	<TextView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/gobutton"
	    android:layout_toRightOf="@id/textFPS1"
	    android:layout_marginTop="10px"
	    android:layout_marginLeft="10px"
	    android:id="@+id/textFPS2"
	/>
	
	<TextView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/gobutton"
	    android:layout_toRightOf="@id/textFPS2"
	    android:layout_marginTop="10px"
	    android:layout_marginLeft="10px"
	    android:id="@+id/textFPS3"
	/>
	
	<TextView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/gobutton"
	    android:layout_toRightOf="@id/textFPS3"
	    android:layout_marginTop="10px"
	    android:layout_marginLeft="10px"
	    android:id="@+id/textFPS4"
	/>
	
	<TextView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/gobutton"
	    android:layout_toRightOf="@id/textFPS4"
	    android:layout_marginTop="10px"
	    android:layout_marginLeft="10px"
	    android:id="@+id/textFPS5"
	/>
	
	<TextView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/gobutton"
	    android:layout_toRightOf="@id/textFPS5"
	    android:layout_marginTop="10px"
	    android:layout_marginLeft="10px"
	    android:id="@+id/textFPS6"
	/>
	
	<TextView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/gobutton"
	    android:layout_toRightOf="@id/textFPS6"
	    android:layout_marginTop="10px"
	    android:layout_marginLeft="10px"
	    android:id="@+id/textFPS7"
	/>
	
	<TextView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/gobutton"
	    android:layout_toRightOf="@id/textFPS7"
	    android:layout_marginTop="10px"
	    android:layout_marginLeft="10px"
	    android:id="@+id/textFPS8"
	/>
	
	<TextView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/textFPS1"
	    android:layout_marginTop="10px"
	    android:layout_toRightOf="@id/textFPS1"
	    android:visibility="invisible"
	    android:id="@+id/textFPSAverage"
	/>
	
	<ImageView
        android:id="@+id/imageViewLogo"
        android:layout_width="85px"
        android:layout_height="85px" 
        android:layout_alignParentRight="true"
        />

</RelativeLayout>